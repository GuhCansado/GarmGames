                                <uie:ToolbarButton text="&#10;" name="AC_CLONE" tooltip="Clone the selected Data Entity" view-data-key="asset_clone" style="flex-grow: 1; -unity-text-align: upper-center; left: 0; border-left-width: 0; border-right-width: 0; background-image: url(&apos;/Assets/Cleverous/VaultCore/Editor/Icons/clone.png&apos;); max-width: 20px; max-height: 16px; margin-right: 3px; width: auto; -unity-background-scale-mode: scale-to-fit; border-top-left-radius: 1px; border-bottom-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px;" />
                                <uie:ToolbarButton text="&#10;" name="AC_GROUP_REMOVE" tooltip="Remove the selected asset from the Data Group" view-data-key="asset_group_remove" style="flex-grow: 1; -unity-text-align: upper-center; left: 0; border-left-width: 0; border-right-width: 0; background-image: url(&apos;/Assets/Cleverous/VaultCore/Editor/Icons/cyl_sub.png&apos;); max-width: 20px; max-height: 16px; margin-right: 3px; width: auto; -unity-background-scale-mode: scale-to-fit; border-top-left-radius: 1px; border-bottom-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px; background-color: rgb(60, 60, 60);" />
