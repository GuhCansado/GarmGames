                                <uie:ToolbarButton name="GC_DEL" tooltip="Delete the selected group" view-data-key="gc_del" style="flex-grow: 0; -unity-text-align: upper-center; -unity-background-scale-mode: scale-to-fit; background-image: url(&apos;/Assets/Cleverous/VaultCore/Editor/Icons/cyl_del.png&apos;); width: 20px; height: 16px; padding-left: 0; padding-right: 0; padding-top: 0; left: 0; border-left-width: 0; border-right-width: 0; margin-left: 0; border-top-left-radius: 1px; border-bottom-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px; padding-bottom: 0; margin-right: 3px;" />
                                <uie:ToolbarButton text="&#10;" name="GC_RELOAD" tooltip="Reload the DB" view-data-key="gc_reload" style="flex-grow: 1; -unity-text-align: upper-center; left: 0; border-left-width: 0; border-right-width: 0; background-image: url(&apos;/Assets/Cleverous/VaultCore/Editor/Icons/refresh.png&apos;); max-width: 20px; max-height: 16px; border-top-left-radius: 1px; border-bottom-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px; margin-right: 3px; -unity-background-scale-mode: scale-to-fit;" />
                                <uie:ToolbarButton text="&#10;" name="GC_HELP" view-data-key="gc_help" tooltip="Go to Web Documentation" style="flex-grow: 1; -unity-text-align: upper-center; left: 0; border-left-width: 0; border-right-width: 0; background-image: url(&apos;/Assets/Cleverous/VaultCore/Editor/Icons/help.png&apos;); flex-shrink: 1; max-width: 20px; max-height: 16px; border-top-left-radius: 1px; border-bottom-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px; margin-right: 3px; -unity-background-scale-mode: scale-to-fit;" />
