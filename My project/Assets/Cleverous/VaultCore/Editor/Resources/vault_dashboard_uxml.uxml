<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <ui:VisualElement name="DASHBOARD_WRAPPER" view-data-key="vault_dashboard" style="height: 100%; width: 100%; flex-grow: 1; min-width: 700px;">
