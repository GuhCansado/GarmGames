                        <ui:VisualElement name="AC_CONTENT" view-data-key="asset_content" style="flex-grow: 1;" />
                    </ui:VisualElement>
                </Cleverous.VaultDashboard.SplitView>
                <ui:IMGUIContainer name="INSPECT_COLUMN" view-data-key="inspector_column" style="flex-grow: 1; border-left-width: 0; border-right-width: 0; border-top-width: 1px; border-bottom-width: 1px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); flex-shrink: 1; background-image: resource(&apos;cleverous_logo_large&apos;); -unity-background-scale-mode: scale-to-fit; -unity-background-image-tint-color: rgba(255, 255, 255, 0.03);" />
