                        </uie:Toolbar>
                        <ui:VisualElement name="GC_CONTENT" view-data-key="group_content" style="flex-grow: 1;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="ASSET_COLUMN" view-data-key="asset_column" style="border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); flex-grow: 1; width: auto; max-width: none; min-width: 250px; flex-shrink: 1; align-items: stretch; height: 100%;">
